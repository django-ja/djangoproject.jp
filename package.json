{
  "name": "djangoproject.jp",
  "version": "0.0.3",
  "description": "Source code of https://djangoproject.jp/",
  "scripts": {
    "build": "npm run -s install-miyadaiku && miyadaiku-build miya && npm run -s setup-artifacts",
    "install-miyadaiku": "python3.8 -m pip install miyadaiku==1.20",
    "setup-artifacts": "mv miya/outputs/* djangoproject.jp/ && cp -r static/* ./djangoproject.jp/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/django-ja/djangoproject.jp"
  },
  "keywords": [
    "django"
  ],
  "author": "django-ja",
  "contributors": [
    "Kiyohara Hiroki (https://github.com/hirokiky)",
    "Matsumura Masayuki (https://github.com/masmatsum)",
    "Takuya Noguchi <takninnovationresearch@gmail.com> (https://github.com/tnir)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/django-ja/djangoproject.jp/issues"
  },
  "homepage": "https://djangoproject.jp"
}
